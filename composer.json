{
  "name": "sudhaus7/userint2ssiesi",
  "description": "(Sudhaus7) Convert USER_INT generated tags to SSI or ESI statements",
  "version": "0.0.1",
  "type": "typo3-cms-extension",
  "authors": [
    {
      "name": "Frank Berger",
      "role": "Developer",
      "email": "fberger@sudhaus7.de"
    }
  ],
  "replace": {
    "sudhaus7_userint2ssiesi": "self.version"
  },
  "require": {
    "php": "~5.6.0 || ~7.0.0 || ~7.1.0 || ~7.2.0",
    "typo3/cms-frontend": "^7.6.0 || ^8.7.0 || ^9.5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7",
    "typo3/cms-fluid-styled-content": "^7.6.0 || ^8.7.0 || ^9.5.0",
    "helhum/typo3-composer-setup": "^0.5.1",
    "helmich/typo3-typoscript-lint": "^1.4.4",
    "nimut/testing-framework": "^4.0",
    "roave/security-advisories": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "SUDHAUS7\\Sudhaus7Userint2ssiesi\\": "Classes/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SUDHAUS7\\Sudhaus7Userint2ssiesi\\Tests\\": "Tests/"
    }
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "vendor-dir": ".Build/vendor"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.0.x-dev"
    },
    "typo3/cms": {
      "extension-key": "sudhaus7_userint2ssiesi",
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": ".Build/public"
    }
  }
}
